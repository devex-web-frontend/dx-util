{
  "name": "dx-util",
  "version": "0.10.19",
  "description": "Common utilities for dx-projects",
  "main": "./src/",
  "engines": {
    "npm": "^3.0.0"
  },
  "scripts": {
    "postinstall": "bower install",
    "test": "npm run eslint && npm run jest",
    "jest": "jest",
    "eslint": "eslint --config ./node_modules/dx-lint/.eslintrc --ext .js ./src",
    "esdoc": "esdoc -c ./config/esdoc.js",
    "start": "jest --watch",
    "bower": "bower",
    "dx-flow": "dx-flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devex-web-frontend/dx-util.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/devex-web-frontend/dx-util/issues"
  },
  "homepage": "https://github.com/devex-web-frontend/dx-util#readme",
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.0",
    "babel-jest": "12.0.2",
    "babel-plugin-transform-class-properties": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.6.0",
    "bower": "^1.7.9",
    "dx-flow": "github:devex-web-frontend/dx-flow",
    "dx-lint": "github:devex-web-frontend/dx-lint",
    "esdoc": "^0.4.7",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "^2.13.1",
    "eslint-plugin-babel": "^3.3.0",
    "jest-cli": "12.0.2"
  }
}
